<?xml version='1.0' encoding='utf-8'?>
<widget id="net.redirectme.tqserver.cipanxampla" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>CI_Panxampla</name>
    <description>
        Practice on UOC to show Cordova application.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
